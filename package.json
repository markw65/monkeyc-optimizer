{
  "name": "@markw65/monkeyc-optimizer",
  "type": "module",
  "version": "1.0.27",
  "description": "Source to source optimizer for Garmin Monkey C code",
  "main": "build/optimizer.cjs",
  "types": "build/src/optimizer.d.ts",
  "exports": {
    ".": {
      "types": "./build/src/optimizer.d.ts",
      "default": "./build/optimizer.cjs"
    },
    "./*.js": {
      "types": "./build/src/*.d.ts",
      "default": "./build/*.cjs"
    }
  },
  "scripts": {
    "watch": "webpack --mode development --watch",
    "build-debug": "webpack --mode development",
    "build-release": "webpack --mode production",
    "prepack": "webpack --mode production",
    "test": "npm run test-optimized && npm run test-remote",
    "test-remote": "node ./test/test.js --product=pick-one --github",
    "test-optimized": "node test/test.js --run-tests --product=fenix5 --product=fr235 --jungle $(pwd)/test/OptimizerTests/monkey.jungle",
    "test-unopt": "node test/test.js --skipOptimization --run-tests --product=fenix5 --product=fr235 --jungle $(pwd)/test/OptimizerTests/monkey.jungle",
    "test-garmin-opt": "node test/test.js --skipOptimization --garminOptLevel=2 --run-tests --product=fenix5 --product=fr235 --jungle $(pwd)/test/OptimizerTests/monkey.jungle",
    "eslint": "npx eslint ."
  },
  "files": [
    "build/optimizer.cjs",
    "build/util.cjs",
    "build/sdk-util.cjs",
    "build/api.cjs",
    "build/src/*.d.ts"
  ],
  "author": "markw65",
  "license": "MIT",
  "dependencies": {
    "@markw65/prettier-plugin-monkeyc": "^1.0.29"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/prettier": "^2.6.1",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.12.0",
    "extract-zip": "^2.0.1",
    "glob": "^7.2.0",
    "peggy": "^1.2.0",
    "prettier": "^2.6.2",
    "prettier-plugin-pegjs": "^0.5.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "xml2js": "^0.4.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markw65/monkeyc-optimizer.git"
  },
  "keywords": [
    "monkeyc"
  ],
  "bugs": {
    "url": "https://github.com/markw65/monkeyc-optimizer/issues"
  },
  "homepage": "https://github.com/markw65/monkeyc-optimizer#readme"
}
