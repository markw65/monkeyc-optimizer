<layout id="MainLayout">
    <label
        x="center"
        y="5"
        text="@Strings.prompt"
        color="Graphics.COLOR_WHITE"
        justification="Graphics.TEXT_JUSTIFY_CENTER" />
    <bitmap
        id="id_monkey"
        x="center"
        y="30"
        filename="../../resources/images/icon.png" />
    <text-area
        text="test"
        personality="
        prompt_color_dark__title
        prompt_font__title
        prompt_loc__title
        prompt_size__title
    " />
    <drawable
        id="test"
        class="TestDrawable">
        <param name="locX">10</param>
        <param name="locY">10</param>
        <param name="width">10</param>
        <param name="height">10</param>
        <param name="enumValue">TestDrawable.VALUE</param>
    </drawable>
</layout>